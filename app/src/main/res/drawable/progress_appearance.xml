<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10dip" />
            <stroke
                android:width="2dip"
                android:color="#ff000000" />
            <gradient
                android:angle="270"
                android:centerColor="#ff8A6249"
                android:centerY="0.50"
                android:endColor="#ff91694F"
                android:startColor="#ff734B35" />

        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="10dip" />
                <stroke
                    android:width="2dip"
                    android:color="#ff000000" />
                <gradient
                    android:angle="270"
                    android:centerColor="#e6e5e3"
                    android:endColor="#e2dddd"
                    android:startColor="#edede2" />

            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dip" />
                <stroke
                    android:width="2dip"
                    android:color="#ff000000" />
                <gradient
                    android:angle="90"
                    android:endColor="#b62e49"
                    android:startColor="#9b0d1d" />
            </shape>
        </clip>
    </item>
</layer-list>